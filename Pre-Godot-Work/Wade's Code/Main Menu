extends Control

func _ready():
	pass
	
func _process(_delta):
	pass
	
func _on_open_table_pressed():
	print("Opening Table")
	get_tree().change_scene_to_file("res://Table Background/Empty_Table.tscn")

func _on_open_games_pressed():
	print("Opening Games Menu")
	get_tree().change_scene_to_file("res://Tic-Tak-Toe/Tic-Tak-Toe.tscn")
	
func _on_commands_pressed() -> void:
	print("Showing List of Commands")
	
func _on_quit_table_pressed():
	print("Quiting Players Table")
	get_tree().quit()
	
	
